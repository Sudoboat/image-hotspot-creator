    "@mui/icons-material": "^5.13.7",
    "@mui/material": "^5.13.7",
    "@mui/styled-engine-sc": "^5.12.0",
    "clone-deep": "^4.0.1",
    "contentful": "^10.3.1",
    "contentful-management": "^10.38.3",
    "emotion": "10.0.27",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-image-crop": "^10.1.5",
    "react-magic-dropzone": "^1.0.1",
